{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/Index.js",
  "scripts": {
    "dev": "nodemon src/Index.ts",
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/Index.js",
    "seed": "ts-node --transpile-only ./src/seed.ts",
    "prisma": "npx prisma migrate dev --name init",
    "prestart": "npm run build",
    "postinstall": "prisma generate",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "This backend project uses **Node.js**, **TypeScript**, and **Prisma** ORM, with **PostgreSQL** as the database. The project follows best practices for environment-based configuration using a `.env` file.",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.670.0",
    "@prisma/client": "^5.21.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.4.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
